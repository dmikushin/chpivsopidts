		ths@5070400 {
			compatible = "allwinner,sun50i-h6-ths";
			reg = <0x5070400 0x100>;
			interrupts = <0x00 0x0f 0x04>;
			clocks = <0x04 0x59>;
			clock-names = "bus";
			resets = <0x04 0x24>;
			nvmem-cells = <0x46>;
			nvmem-cell-names = "calibration";
			#thermal-sensor-cells = <0x01>;
			phandle = <0x47>;
		};
	};

	thermal-zones {

		cpu-thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x47 0x00>;

			trips {

				cpu_warm {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x48>;
				};

				cpu_hot_pre {
					temperature = <0x13880>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x49>;
				};

				cpu_hot {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x4a>;
				};

				cpu_very_hot_pre {
					temperature = <0x15f90>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x4b>;
				};

				cpu_very_hot {
					temperature = <0x17318>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x4c>;
				};

				cpu_crit {
					temperature = <0x19a28>;
					hysteresis = <0x7d0>;
					type = "critical";
					phandle = <0x87>;
				};
			};

			cooling-maps {

				cpu_warm_limit_cpu {
					trip = <0x48>;
					cooling-device = <0x09 0xffffffff 0x02>;
				};

				cpu_hot_pre_limit_cpu {
					trip = <0x49>;
					cooling-device = <0x09 0x02 0x03>;
				};

				cpu_hot_limit_cpu {
					trip = <0x4a>;
					cooling-device = <0x09 0x03 0x04>;
				};

				cpu_very_hot_pre_limit_cpu {
					trip = <0x4b>;
					cooling-device = <0x09 0x05 0x06>;
				};

				cpu_very_hot_limit_cpu {
					trip = <0x4c>;
					cooling-device = <0x09 0x07 0xffffffff>;
				};
			};
		};

		gpu-thermal {
			polling-delay-passive = <0x00>;
			polling-delay = <0x00>;
			thermal-sensors = <0x47 0x01>;
		};
	};

