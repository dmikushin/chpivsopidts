/dts-v1/;

/ {
	interrupt-parent = <0x01>;
	#address-cells = <0x01>;
	#size-cells = <0x01>;
	model = "OrangePi 3";
	compatible = "xunlong,orangepi-3\0allwinner,sun50i-h6";

	ac200_clk {
		compatible = "pwm-clock";
		#clock-cells = <0x00>;
		clock-frequency = <0x16e3600>;
		pinctrl-names = "default";
		pinctrl-0 = <0x02>;
		pwms = <0x03 0x01 0x2a 0x00>;
		status = "disabled";
		phandle = <0x2a>;
	};

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		cpu@0 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x00>;
			enable-method = "psci";
			clocks = <0x04 0x15>;
			clock-latency-ns = <0x3b9b0>;
			operating-points-v2 = <0x05>;
			#cooling-cells = <0x02>;
			cpu-supply = <0x06>;
			phandle = <0x09>;
		};

		cpu@1 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x01>;
			enable-method = "psci";
			clocks = <0x04 0x15>;
			clock-latency-ns = <0x3b9b0>;
			operating-points-v2 = <0x05>;
			#cooling-cells = <0x02>;
			phandle = <0x0a>;
		};

		cpu@2 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x02>;
			enable-method = "psci";
			clocks = <0x04 0x15>;
			clock-latency-ns = <0x3b9b0>;
			operating-points-v2 = <0x05>;
			#cooling-cells = <0x02>;
			phandle = <0x0b>;
		};

		cpu@3 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x03>;
			enable-method = "psci";
			clocks = <0x04 0x15>;
			clock-latency-ns = <0x3b9b0>;
			operating-points-v2 = <0x05>;
			#cooling-cells = <0x02>;
			phandle = <0x0c>;
		};
	};

	opp_table {
		compatible = "allwinner,sun50i-h6-operating-points";
		nvmem-cells = <0x07>;
		opp-shared;
		phandle = <0x05>;

		opp@480000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x1c9c3800>;
			opp-microvolt-speed0 = <0xd6d80>;
			opp-microvolt-speed1 = <0xc8320>;
			opp-microvolt-speed2 = <0xc8320>;
		};

		opp@720000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x2aea5400>;
			opp-microvolt-speed0 = <0xd6d80>;
			opp-microvolt-speed1 = <0xc8320>;
			opp-microvolt-speed2 = <0xc8320>;
		};

		opp@816000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x30a32c00>;
			opp-microvolt-speed0 = <0xd6d80>;
			opp-microvolt-speed1 = <0xc8320>;
			opp-microvolt-speed2 = <0xc8320>;
		};

		opp@888000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x34edce00>;
			opp-microvolt-speed0 = <0xd6d80>;
			opp-microvolt-speed1 = <0xc8320>;
			opp-microvolt-speed2 = <0xc8320>;
		};

		opp@1080000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x405f7e00>;
			opp-microvolt-speed0 = <0xe57e0>;
			opp-microvolt-speed1 = <0xd6d80>;
			opp-microvolt-speed2 = <0xd6d80>;
		};

		opp@1320000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x4ead9a00>;
			opp-microvolt-speed0 = <0xf4240>;
			opp-microvolt-speed1 = <0xe57e0>;
			opp-microvolt-speed2 = <0xe57e0>;
		};

		opp@1488000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x58b11400>;
			opp-microvolt-speed0 = <0x102ca0>;
			opp-microvolt-speed1 = <0xf4240>;
			opp-microvolt-speed2 = <0xf4240>;
		};

		opp@1640000000 {
			opp-hz = <0x00 0x61c06a00>;
			opp-microvolt = <0x11b340 0x11b340 0x11b340>;
			clock-latency-ns = <0x3b9b0>;
		};

		opp@1800000000 {
			clock-latency-ns = <0x3b9b0>;
			opp-hz = <0x00 0x6b49d200>;
			opp-microvolt-speed0 = <0x11b340>;
			opp-microvolt-speed1 = <0x10c8e0>;
			opp-microvolt-speed2 = <0x10c8e0>;
		};
	};

	display-engine {
		compatible = "allwinner,sun50i-h6-display-engine";
		allwinner,pipelines = <0x08>;
		status = "okay";
		phandle = <0x4f>;
	};

	osc24M_clk {
		#clock-cells = <0x00>;
		compatible = "fixed-clock";
		clock-frequency = <0x16e3600>;
		clock-output-names = "osc24M";
		phandle = <0x15>;
	};

	ext_osc32k_clk {
		#clock-cells = <0x00>;
		compatible = "fixed-clock";
		clock-frequency = <0x8000>;
		clock-output-names = "ext_osc32k";
		phandle = <0x42>;
	};

	pmu {
		compatible = "arm,cortex-a53-pmu";
		interrupts = <0x00 0x8c 0x04 0x00 0x8d 0x04 0x00 0x8e 0x04 0x00 0x8f 0x04>;
		interrupt-affinity = <0x09 0x0a 0x0b 0x0c>;
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	timer {
		compatible = "arm,armv8-timer";
		interrupts = <0x01 0x0d 0xf04 0x01 0x0e 0xf04 0x01 0x0b 0xf04 0x01 0x0a 0xf04>;
	};

	sound {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,name = "allwinner-hdmi";
		simple-audio-card,mclk-fs = <0x100>;
		phandle = <0x50>;

		simple-audio-card,codec {
			sound-dai = <0x0d>;
		};

		simple-audio-card,cpu {
			sound-dai = <0x0e>;
			dai-tdm-slot-num = <0x02>;
			dai-tdm-slot-width = <0x20>;
		};
	};

	soc {
		compatible = "simple-bus";
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;

		bus@1000000 {
			compatible = "allwinner,sun50i-h6-de3\0allwinner,sun50i-a64-de2";
			reg = <0x1000000 0x400000>;
			allwinner,sram = <0x0f 0x01>;
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			ranges = <0x00 0x1000000 0x400000>;

			clock@0 {
				compatible = "allwinner,sun50i-h6-de3-clk";
				reg = <0x00 0x10000>;
				clocks = <0x04 0x1d 0x04 0x1e>;
				clock-names = "mod\0bus";
				resets = <0x04 0x01>;
				#clock-cells = <0x01>;
				#reset-cells = <0x01>;
				phandle = <0x10>;
			};

			mixer@100000 {
				compatible = "allwinner,sun50i-h6-de3-mixer-0";
				reg = <0x100000 0x100000>;
				clocks = <0x10 0x00 0x10 0x06>;
				clock-names = "bus\0mod";
				resets = <0x10 0x00>;
				phandle = <0x08>;

				ports {
					#address-cells = <0x01>;
					#size-cells = <0x00>;

					port@1 {
						reg = <0x01>;
						phandle = <0x51>;

						endpoint {
							remote-endpoint = <0x11>;
							phandle = <0x39>;
						};
					};
				};
			};
		};

		video-codec@1c0e000 {
			compatible = "allwinner,sun50i-h6-video-engine";
			reg = <0x1c0e000 0x2000>;
			clocks = <0x04 0x26 0x04 0x25 0x04 0x36>;
			clock-names = "ahb\0mod\0ram";
			resets = <0x04 0x05>;
			interrupts = <0x00 0x59 0x04>;
			allwinner,sram = <0x12 0x01>;
		};

		opp-table2 {
			compatible = "operating-points-v2";
			phandle = <0x13>;

			opp00 {
				opp-hz = <0x00 0x2d0fa500>;
				opp-microvolt = <0xfde80>;
			};

			opp01 {
				opp-hz = <0x00 0x25317c00>;
				opp-microvolt = <0xe7ef0>;
			};

			opp02 {
				opp-hz = <0x00 0x22551000>;
				opp-microvolt = <0xe30d0>;
			};

			opp03 {
				opp-hz = <0x00 0x202fbf00>;
				opp-microvolt = <0xde2b0>;
			};

			opp04 {
				opp-hz = <0x00 0x1e0a6e00>;
				opp-microvolt = <0xd9490>;
			};

			opp05 {
				opp-hz = <0x00 0x1b2e0200>;
				opp-microvolt = <0xd4670>;
			};

			opp06 {
				opp-hz = <0x00 0x19bfcc00>;
				opp-microvolt = <0xd1f60>;
			};

			opp07 {
				opp-hz = <0x00 0x1908b100>;
				opp-microvolt = <0xcf850>;
			};

			opp08 {
				opp-hz = <0x00 0x18519600>;
				opp-microvolt = <0xcd140>;
			};

			opp09 {
				opp-hz = <0x00 0x16e36000>;
				opp-microvolt = <0xcaa30>;
			};

			opp10 {
				opp-hz = <0x00 0x15752a00>;
				opp-microvolt = <0xc8320>;
			};

			opp11 {
				opp-hz = <0x00 0x1406f400>;
				opp-microvolt = <0xc5c10>;
			};

			opp12 {
				opp-hz = <0x00 0x1298be00>;
				opp-microvolt = <0xc5c10>;
			};

			opp13 {
				opp-hz = <0x00 0xfbc5200>;
				opp-microvolt = <0xc5c10>;
			};

			opp14 {
				opp-hz = <0x00 0xcdfe600>;
				opp-microvolt = <0xc5c10>;
			};
		};

		gpu@1800000 {
			compatible = "allwinner,sun50i-h6-mali\0arm,mali-t720";
			reg = <0x1800000 0x4000>;
			interrupts = <0x00 0x54 0x04 0x00 0x55 0x04 0x00 0x53 0x04>;
			interrupt-names = "job\0mmu\0gpu";
			clocks = <0x04 0x21 0x04 0x22>;
			clock-names = "core\0bus";
			resets = <0x04 0x03>;
			operating-points-v2 = <0x13>;
			status = "okay";
			mali-supply = <0x14>;
			phandle = <0x52>;
		};

		syscon@3000000 {
			compatible = "allwinner,sun50i-h6-system-control\0allwinner,sun50i-a64-system-control";
			reg = <0x3000000 0x1000>;
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			ranges;
			phandle = <0x2c>;

			sram@28000 {
				compatible = "mmio-sram";
				reg = <0x28000 0x1e000>;
				#address-cells = <0x01>;
				#size-cells = <0x01>;
				ranges = <0x00 0x28000 0x1e000>;
				phandle = <0x53>;

				sram-section@0 {
					compatible = "allwinner,sun50i-h6-sram-c\0allwinner,sun50i-a64-sram-c";
					reg = <0x00 0x1e000>;
					phandle = <0x0f>;
				};
			};

			sram@1a00000 {
				compatible = "mmio-sram";
				reg = <0x1a00000 0x200000>;
				#address-cells = <0x01>;
				#size-cells = <0x01>;
				ranges = <0x00 0x1a00000 0x200000>;
				phandle = <0x54>;

				sram-section@0 {
					compatible = "allwinner,sun50i-h6-sram-c1\0allwinner,sun4i-a10-sram-c1";
					reg = <0x00 0x200000>;
					phandle = <0x12>;
				};
			};
		};

		clock@3001000 {
			compatible = "allwinner,sun50i-h6-ccu";
			reg = <0x3001000 0x1000>;
			clocks = <0x15 0x16 0x00 0x16 0x02>;
			clock-names = "hosc\0losc\0iosc";
			#clock-cells = <0x01>;
			#reset-cells = <0x01>;
			phandle = <0x04>;
		};

		dma-controller@3002000 {
			compatible = "allwinner,sun50i-h6-dma";
			reg = <0x3002000 0x1000>;
			interrupts = <0x00 0x2b 0x04>;
			clocks = <0x04 0x2b 0x04 0x35>;
			clock-names = "bus\0mbus";
			dma-channels = <0x10>;
			dma-requests = <0x2e>;
			resets = <0x04 0x08>;
			#dma-cells = <0x01>;
			phandle = <0x21>;
		};

		interrupt-controller@3021000 {
			compatible = "arm,gic-400";
			reg = <0x3021000 0x1000 0x3022000 0x2000 0x3024000 0x2000 0x3026000 0x2000>;
			interrupts = <0x01 0x09 0xf04>;
			interrupt-controller;
			#interrupt-cells = <0x03>;
			phandle = <0x01>;
		};

		efuse@3006000 {
			compatible = "allwinner,sun50i-h6-sid";
			reg = <0x3006000 0x400>;
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			phandle = <0x55>;

			ephy_calib@2c {
				reg = <0x2c 0x02>;
				phandle = <0x2b>;
			};

			speed@1c {
				reg = <0x1c 0x04>;
				phandle = <0x07>;
			};

			thermal-sensor-calibration@14 {
				reg = <0x14 0x06>;
				phandle = <0x46>;
			};
		};

		watchdog@30090a0 {
			compatible = "allwinner,sun50i-h6-wdt\0allwinner,sun6i-a31-wdt";
			reg = <0x30090a0 0x20>;
			interrupts = <0x00 0x32 0x04>;
			clocks = <0x15>;
			status = "disabled";
			phandle = <0x56>;
		};

		pwm@300a000 {
			compatible = "allwinner,sun50i-h6-pwm";
			reg = <0x300a000 0x400>;
			clocks = <0x15 0x04 0x32>;
			clock-names = "pwm\0bus";
			resets = <0x04 0x0e>;
			#pwm-cells = <0x03>;
			status = "disabled";
			phandle = <0x03>;
		};

		pinctrl@300b000 {
			compatible = "allwinner,sun50i-h6-pinctrl";
			reg = <0x300b000 0x400>;
			interrupts = <0x00 0x33 0x04 0x00 0x35 0x04 0x00 0x36 0x04 0x00 0x3b 0x04>;
			clocks = <0x04 0x1a 0x15 0x16 0x00>;
			clock-names = "apb\0hosc\0losc";
			gpio-controller;
			#gpio-cells = <0x03>;
			interrupt-controller;
			#interrupt-cells = <0x03>;
			vcc-pc-supply = <0x17>;
			vcc-pd-supply = <0x18>;
			vcc-pg-supply = <0x19>;
			phandle = <0x1b>;

			rgmii-pins {
				pins = "PD0\0PD1\0PD2\0PD3\0PD4\0PD5\0PD7\0PD8\0PD9\0PD10\0PD11\0PD12\0PD13\0PD19\0PD20";
				function = "emac";
				drive-strength = <0x28>;
				phandle = <0x2d>;
			};

			hdmi-pins {
				pins = "PH8\0PH9\0PH10";
				function = "hdmi";
				phandle = <0x36>;
			};

			i2c0-pins {
				pins = "PD25\0PD26";
				function = "i2c0";
				phandle = <0x26>;
			};

			i2c1-pins {
				pins = "PH5\0PH6";
				function = "i2c1";
				phandle = <0x27>;
			};

			i2c2-pins {
				pins = "PD23\0PD24";
				function = "i2c2";
				phandle = <0x28>;
			};

			mmc0-pins {
				pins = "PF0\0PF1\0PF2\0PF3\0PF4\0PF5";
				function = "mmc0";
				drive-strength = <0x1e>;
				bias-pull-up;
				phandle = <0x1a>;
			};

			mmc1-pins {
				pins = "PG0\0PG1\0PG2\0PG3\0PG4\0PG5";
				function = "mmc1";
				drive-strength = <0x1e>;
				bias-pull-up;
				phandle = <0x1c>;
			};

			mmc2-pins {
				pins = "PC1\0PC4\0PC5\0PC6\0PC7\0PC8\0PC9\0PC10\0PC11\0PC12\0PC13\0PC14";
				function = "mmc2";
				drive-strength = <0x1e>;
				bias-pull-up;
				phandle = <0x20>;
			};

			spdif-tx-pin {
				pins = "PH7";
				function = "spdif";
				phandle = <0x31>;
			};

			spi0-pins {
				pins = "PC2\0PC3\0PC0\0PC5";
				function = "spi0";
				phandle = <0x57>;
			};

			spi1-pins {
				pins = "PH5\0PH6\0PH4\0PH3";
				function = "spi1";
				phandle = <0x22>;
			};

			uart0-ph-pins {
				pins = "PH0\0PH1";
				function = "uart0";
				phandle = <0x23>;
			};

			uart1-pins {
				pins = "PG6\0PG7";
				function = "uart1";
				phandle = <0x24>;
			};

			uart1-rts-cts-pins {
				pins = "PG8\0PG9";
				function = "uart1";
				phandle = <0x25>;
			};

			uart2-pins {
				pins = "PD19\0PD20";
				function = "uart2";
				phandle = <0x58>;
			};

			uart2-rts-cts-pins {
				pins = "PD21\0PD22";
				function = "uart2";
				phandle = <0x59>;
			};

			uart3-pins {
				pins = "PD23\0PD24";
				function = "uart3";
				phandle = <0x5a>;
			};

			uart3-rts-cts-pins {
				pins = "PD25\0PD26";
				function = "uart3";
				phandle = <0x5b>;
			};

			i2c3-pins {
				pins = "PB17\0PB18";
				function = "i2c3";
				phandle = <0x29>;
			};

			pwm1-pin {
				pins = "PB19";
				function = "pwm1";
				phandle = <0x02>;
			};

			bt_pwr_pin@0 {
				pins = "PM4";
				function = "gpio_out";
				phandle = <0x4e>;
			};
		};

		mmc@4020000 {
			compatible = "allwinner,sun50i-h6-mmc\0allwinner,sun50i-a64-mmc";
			reg = <0x4020000 0x1000>;
			clocks = <0x04 0x43 0x04 0x40>;
			clock-names = "ahb\0mmc";
			resets = <0x04 0x12>;
			reset-names = "ahb";
			interrupts = <0x00 0x23 0x04>;
			pinctrl-names = "default";
			pinctrl-0 = <0x1a>;
			status = "okay";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			vmmc-supply = <0x18>;
			cd-gpios = <0x1b 0x05 0x06 0x01>;
			bus-width = <0x04>;
			phandle = <0x5c>;
		};

		mmc@4021000 {
			compatible = "allwinner,sun50i-h6-mmc\0allwinner,sun50i-a64-mmc";
			reg = <0x4021000 0x1000>;
			clocks = <0x04 0x44 0x04 0x41>;
			clock-names = "ahb\0mmc";
			resets = <0x04 0x13>;
			reset-names = "ahb";
			interrupts = <0x00 0x24 0x04>;
			pinctrl-names = "default";
			pinctrl-0 = <0x1c>;
			status = "okay";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			vmmc-supply = <0x1d>;
			vqmmc-supply = <0x19>;
			mmc-pwrseq = <0x1e>;
			bus-width = <0x04>;
			non-removable;
			phandle = <0x5d>;

			sdio-wifi@1 {
				reg = <0x01>;
				compatible = "brcm,bcm4329-fmac";
				interrupt-parent = <0x1f>;
				interrupts = <0x01 0x00 0x08>;
				interrupt-names = "host-wake";
				phandle = <0x5e>;
			};
		};

		mmc@4022000 {
			compatible = "allwinner,sun50i-h6-emmc\0allwinner,sun50i-a64-emmc";
			reg = <0x4022000 0x1000>;
			clocks = <0x04 0x45 0x04 0x42>;
			clock-names = "ahb\0mmc";
			resets = <0x04 0x14>;
			reset-names = "ahb";
			interrupts = <0x00 0x25 0x04>;
			pinctrl-names = "default";
			pinctrl-0 = <0x20>;
			status = "okay";
